@{    var IsLangVN = FBabymartvn.IsLangVN();
}
<!DOCTYPE HTML PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<!-- InstanceBegin template="/Templates/tamplate.dwt" codeOutsideHTMLIsLocked="false" -->
<head>
    <meta name="geo.region" content="VN-SG" />
    <meta name="geo.placename" content="Ho Chi Minh, Tan Phu" />
    <meta name="geo.position" content="10.794316;106.630443" />
    <meta name="ICBM" content="10.794316, 106.630443" />
    <meta name="DC.title" content="babymart, Tan Phu, TP HCM" />
    <!-- meta tags start -->
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="Keywords" content="shopey, premium, mobile, template, HTML, CSS" />
    <meta name="Description" content="Premium mobile HTML/CSS template." />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" />
    @RenderSection("SEOContent", required: false)
    <title>Babymart.vn</title>
    <link rel="canonical" href="@HttpContext.Current.Request.Url.AbsoluteUri" />
    <link rel="shortcut icon" href="images/common/favicon.ico" type="image/x-icon" />
    <link href='https://fonts.googleapis.com/css?family=Droid+Serif:400,700,400italic,700italic' rel='stylesheet' type='text/css' />
    <link href="~/Content/mobile/css/all-style.css" rel="stylesheet" type="text/css" media="all" />
    <link href="~/Content/forntpage/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="stylesheet" type="text/css" href="~/Content/forntpage/css/jquery-ui.css" />
</head>
<body>
    <!-- JavaScript files start -->
    <!-- website wrapper start -->
    @{Html.RenderAction("Menu", "Common");}
    <!-- website wrapper starts -->
    <div class="websiteWrapper">
        <!-- page wrapper starts -->
        <div class="pageWrapper homePageWrapper">
            <!-- main menu outer wrapper starts -->
            @Html.Partial("Partial/_TopBar.Mobile")
            <!-- main menu outer wrapper ends -->
            <!-- shopping cart wrapper start -->
            @{Html.RenderAction("Cart", "Common");}
            <!-- shopping cart wrapper ends -->
            <!-- header outer wrapper starts -->
            @Html.Partial("Partial/_Header.Mobile")

            <!-- header outer wrapper ends -->
            <!-- page content wrapper starts -->
            @*<div class="pageContentWrapper" style="background-image:url('https://babymart.vn/Images/hinhdulieu/upload/snow_vect.jpg')">*@
            <div class="pageContentWrapper">
                <!-- shop search form wrapper starts -->
                @Html.Partial("Partial/_Search.Mobile")
                <div id="danhmuc" style="margin-bottom:30px;">
                    <div id="menuclick"><span>@(IsLangVN ? "DANH MỤC SẢN PHẨM" : "CATEGORY") </span><img src="/Images/mobile/images/common/mainMenuButtonBg.png" style="float:right;margin-top: 10px;right:10px;" /></div>
                </div>
                @RenderBody()
            </div>
            <!-- accordion wrapper ends -->
            <!-- page content wrapper ends -->

            <div class="footerDecoration"></div>

            <!-- footer wrapper starts -->
            @*<div class="footerWrapper" style="background: #df0000 url('https://babymart.vn/Images/hinhdulieu/upload/footer-mobile-newyear-2017.png') 0 top no-repeat;background-size: 100%;color: white;">*@
            <div class="footerWrapper" style="background-size: 100%;color: white;">
                @{Html.RenderAction("Footer", "Common");}
            </div>
            <!-- footer wrapper ends -->
        </div>
    </div>
    <!-- page wrapper ends -->
    <!-- JavaScript files start -->
    <script type="text/javascript" src="~/Scripts/mobile/js/jquery.min.js"></script>
    <script type="text/javascript" src="~/Scripts/mobile/js/all-script.js"></script>
    <script src="~/Scripts/forntpage/custom.js" type="text/javascript"></script>
    <script src="~/Scripts/forntpage/jquery-ui.min.js"></script>
    @RenderSection("scriptsstart", required: false)



    <script>

        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-40175858-1', 'auto');
        ga('send', 'pageview');


        @*$(function () {
            $.widget("app.autocomplete", $.ui.autocomplete, {
                options: {
                    highlightClass: "ui-state-highlight"
                },
                _renderItem: function (ul, item) {
                    var re = new RegExp("(" + this.term + ")", "gi"),
