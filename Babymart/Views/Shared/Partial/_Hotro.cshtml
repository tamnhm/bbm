@model IList<Babymart.Models.shop_page>
@{
    Layout = null;
    var IsLangVN = FBabymartvn.IsLangVN();    
}
@foreach (var item in Model)
{
    <li><a href="@("/bai-viet/"+item.url+".html")">@(FBabymartvn.IsLangVNHasValue(item.tieude_us) ? item.tieude : item.tieude_us)<i class="fa fa-angle-right"></i></a></li>
} 